@model AlertMessage
@*resultMesasgeın işi: kendine gelen alertMessageı işleyerek 
ona gore bir alert kutusu olusturmak*@
<div class="row">
    <div class="col-md-12">
        <div class="alert alert-@Model.AlertType">
            <h4 class="alert-title">@Model.Title</h4>
            <p>@Model.Message</p>
        </div>
    </div>
</div>